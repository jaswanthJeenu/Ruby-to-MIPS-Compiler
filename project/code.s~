sw $s6,0($s2)
sw $s6,temp61
addi $s6,$0,1
add $s7,$s7,$s6
sw $s7,temp21
j label1
sw $s4,temp31
sw $s3,a1
label2: 
lw $s7,temp21
move $s6,$s7
sw $s7,temp21
sw $s6,i1
la $s6,str0
